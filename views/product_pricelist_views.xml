<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_product_pricelist_tree_inherit" model="ir.ui.view">
        <field name="name">product.pricelist.tree.inherit</field>
        <field name="model">product.pricelist</field>
        <field name="inherit_id" ref="product.product_pricelist_view_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="inside">
                <header>
                    <button string="Import" type="action" name="%(action_pricelist_import)d" class="btn-primary"/>
                    <button string="Export" type="action" name="%(action_pricelist_export)d" class="btn-secondary"/>
                </header>
            </xpath>
        </field>
    </record>
</odoo>