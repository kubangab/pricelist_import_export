<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_pricelist_import_export_wizard" model="ir.ui.view">
        <field name="name">pricelist.import.export.wizard.form</field>
        <field name="model">pricelist.import_export</field>
        <field name="arch" type="xml">
            <form string="Import Pricelist">
                <group>
                    <field name="pricelist_id" invisible="1"/>
                    <field name="action_type" invisible="1"/>
                    <field name="file" filename="file_name"/>
                    <field name="file_name" invisible="1"/>
                </group>
                <footer>
                    <button string="Import" name="execute_action" type="object" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>